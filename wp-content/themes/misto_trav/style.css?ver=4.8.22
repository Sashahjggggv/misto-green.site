/*Phone header*/
.phones-header {position: absolute;right: 16%;top: 7px;font-size: 16px;text-transform: uppercase;font-family: 'bold';padding-right: 51px;}
.phones-header .toggle {position: absolute;right: -10000px;top: 100%;background: #fff;box-shadow: 5px 9px 20px rgba(24,24,24,.2);width: auto;padding: 20px 53px 20px 20px;opacity: 0;transform: scale(.95) translateY(10px);-webkit-transform: scale(.95) translateY(10px);transition: opacity .25s, transform .25s, right 0s .25s;-webkit-transition: opacity .25s, -webkit-transform .25s, right 0s .25s;transform-origin: 100% 0;-webkit-transform-origin: 100% 0;}
.phones-header .icon {cursor: pointer;transition: opacity .3s, transform .3s;-webkit-transition: opacity .3s, -webkit-transform .3s;}
html:not(.mobile ) .phones-header .icon:hover {opacity: .5;}
html:not(.mobile ) .phones-header:hover .toggle, .phones-header.active .toggle {opacity: 1;transform: scale(1) translateY(0px);-webkit-transform: scale(1) translateY(0px);right: -20px;transition: opacity .25s, transform .25s, right 0s;-webkit-transition: opacity .25s, -webkit-transform .25s, right 0s;}
.phones-header .toggle li {white-space: nowrap;height: auto;line-height: 1em;}
.phones-header .button-close {right: 5px;position: absolute;top: 50%;margin-top: -18px;}
html:not(.mobile ) .phones-header li a:hover {color: #6abf4a;}
html:not(.mobile ) .phones-header .button-close{display: none;}
html:not(.mobile ) .phones-header .toggle{padding: 20px;}
@media (min-width: 768px) and (max-width: 1690px) {
    .phones-header{right: 220px;}
}
@media (min-width: 340px) and (max-width: 767px) {
    .phones-header{right: 95px;}
}
@media (max-width: 399px) {
    .phones-header{display: none;}
}
